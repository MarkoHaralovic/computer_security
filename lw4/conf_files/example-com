$TTL   60000
@   IN   SOA     example.com. root.example.com (
         2023031501 ; serial
         28800 ; refresh
         14400 ; retry
         3600000 ; expire
         0 ; default_ttl
)

example.com.          IN  NS dns.example.com.
dns.example.com.      IN   A 198.51.100.10
www.example.com.      IN   A 198.51.100.10
server.example.com.   IN   A 198.51.100.10

